{
    "name": "phower/http",
    "description": "A PSR-7 implementation.",
    "type": "library",
    "require": {
        "php": "^5.6 || ^7.0",
        "psr/http-message": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.2",
        "squizlabs/php_codesniffer": "^2.5",
        "symfony/var-dumper": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Phower\\Http\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PhowerTest\\Http\\": "tests/"
        }
    },
